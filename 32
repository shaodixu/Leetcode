class Solution(object):
    
        
    def longestValidParentheses(self, s):
        longest, last, indices = 0, -1, []
        for i in xrange(len(s)):
            if s[i] == '(':
                indices.append(i)
            else:
                if not indices:
                    last = i
                else:
                    indices.pop()
                    if not indices:
                        longest = max(longest, i - last)
                    else:
                        longest = max(longest, i - indices[-1])
        return longest       
    
   
   
