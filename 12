class Solution(object):
    def intToRoman(self, num):
        """
        :type num: int
        :rtype: str
        """
        d1 = [1000,900,500,400,100,90,50,40,10,9,5,4,1]
        d2 = ["M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I"]
        
        res = ""
        for i in range(len(d1)):
            while num>=d1[i]:
                num -= d1[i]
                res += d2[i]
        return res

        
