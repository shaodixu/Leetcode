class Solution(object):
    def lengthOfLastWord(self, s):
        """
        :type s: str
        :rtype: int
        """
        return len(s.strip().split(" ")[-1])




    def lengthOfLastWord(self, s):  
        k = 0
        s = s.strip()
        for i in range(len(s)):
            if s[i] != ' ':
                k += 1
            elif s[i] == ' ':
                k = 0
        return k
