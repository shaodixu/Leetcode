class Solution(object):
    def firstUniqChar(self, s):
        """
        :type s: str
        :rtype: int
        """
        lookup = {}
        for c in s:
            lookup[c] = lookup[c]+1 if c in lookup else 1
        for i in range(len(s)):
            if lookup[s[i]] == 1:
                return i
        return -1
