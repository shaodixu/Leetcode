class Solution(object):
    def getRow(self, rowIndex):
        row= [[1]]
        for i in range(1,rowIndex+1):
            row[0].append(0)
            for j in range(i,0,-1):
                if j==i:
                    row[0][j]=1
                else:
                    row[0][j]+=row[0][j-1]
        
        return row[0]
        
