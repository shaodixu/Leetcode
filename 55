class Solution(object):
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        ma = 0
        for i in range(len(nums)):
            if i > ma:
                return False
            ma = max(nums[i] + i, ma)
        return True
        


class Solution:
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        if len(nums) <2:
            return True
        i = 0
        reach = 0
        while i < len(nums) and i <= reach:
            reach = max(reach, nums[i] + i)
            
            i += 1
        if reach >= len(nums) - 1:
            return True
        else:
            return False
