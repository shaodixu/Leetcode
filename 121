class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        if len(prices)<2:
            return 0
        mi = prices[0]
        res = 0
        for price in prices:
            mi= min(mi, price)
            res  = max(res, price-mi)
        return res
