class Solution(object):
    def countPrimes(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n < 2:
            return 0
        res = n*[True]
        res[0] = False
        res[1] = False
        for i in range(2, int(math.sqrt(n))+1):
            if res[i]:
                res[i*i:n:i] = [False]*len(res[i*i:n:i])
        return sum(res)
