class Solution(object):
    def isHappy(self, n):
        """
        :type n: int
        :rtype: bool
        """
        visited = set([])
        while(n not in visited):
            visited.add(n)
            _sum = 0
            
            while(n):
                _sum += (n % 10)**2
                n /= 10
            if(_sum == 1):
                return True
            n = _sum
        return False
        
