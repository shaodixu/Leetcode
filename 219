class Solution(object):
    def containsNearbyDuplicate(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: bool
        """
        lookup = {}
        for i, num in enumerate(nums):
            if num in lookup:
                if i - lookup[num] <= k:
                    return True
            lookup[num] = i
        return False
    
    
   
